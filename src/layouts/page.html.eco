---
layout: default
---

<!-- Breadcrumbs -->
  <ul class="breadcrumb">
    <% for crumb in @getBreadcrumbs(): %>
      <li><a href="<%= @getUrl(crumb.href) %>"><%= crumb.name %></a> <span class="divider"> &#8594; </span></li>
    <% end %>
    <li class="active"><%= @document.title %></li>
</ul>

<article id="page" class="page">
  <div class="page-content"><%- @content %></div>
</article>