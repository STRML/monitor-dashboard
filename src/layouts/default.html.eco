<!DOCTYPE html>
<html lang="en">
  <head>

    <!-- Meta -->
    <meta charset="utf-8" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <%- @getBlock('meta').toHTML() %>
    <meta name="viewport" content="width=device-width" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="description" content="<%= @getPreparedDescription() %>" />
    <meta name="keywords" content="<%= @getPreparedKeywords() %>" />
    <meta name="author" content="<%= @site.author or '' %>" />
    <link rel="icon" href="<%- @getUrl('/favicon.ico') %>" type="image/x-icon">
    <title><%= @getPreparedTitle() %></title>

    <!-- Shims: IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script async src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->

    <!-- Styles -->
    <%- @getBlock('styles').add(@getUrl(["/css/bootstrap.css","/css/font-awesome.css","/css/site.css"])).toHTML() %>
  </head>

  <body data-title="<%= @getPreparedTitle() %>">

    <!-- Navbar -->
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="<%- @getUrl('/index.html') %>">
            <img src="<%- @getUrl('/img/monitor.jpg') %>" class="logo"></img>
	    Monitor Dashboard
	  </a>
          <div class="nav-collapse collapse">
            <ul class="nav pull-right">
              <li><a href="<%- @getUrl('/pages/getting-started.html') %>">Getting Started</a></li>
              <li class="dropdown">
                <a href="" class="dropdown-toggle" data-toggle="dropdown">Overview <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="<%- @getUrl('/pages/overview/dashboards.html') %>">Dashboards</a></li>
                  <li><a href="<%- @getUrl('/pages/overview/tours.html') %>">Tours</a></li>
                  <li><a href="<%- @getUrl('/pages/overview/apps.html') %>">Apps</a></li>
                </ul>
              </li>
	      <!--
              <li class="dropdown">
                <a href="" class="dropdown-toggle" data-toggle="dropdown">Developers <b class="caret"></b></a>
                <ul class="dropdown-menu">
                  <li><a href="<%- @getUrl('/pages/developers/probes.html') %>">Building Data Probes</a></li>
                  <li><a href="<%- @getUrl('/pages/developers/views.html') %>">Building Monitor Views</a></li>
                  <li class="divider"></li>
                  <li><a href="<%- @getUrl('/pages/developers/packaging.html') %>">Packaging Your App</a></li>
		</ul>
	      </li>
              <li><a href="<%- @getUrl('/blog/index.html') %>">Blog</a></li>
	      -->
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">
      <section id="content" class="content">
        <%- @content %>
      </section>
    </div> <!-- /container -->

    <!-- Footer spans entire bottom section -->
    <footer class="navbar navbar-inverse navbar-bottom">
      <div class="navbar-inner container">
        <ul class="nav footer-refs pull-left">
          <li><a href="https://github.com/lorenwest/monitor-dashboard">Copyright &copy;2010-2014</a></li>
          <li><a href="https://github.com/lorenwest/monitor-dashboard/blob/master/LICENSE">May be freely used and distributed under the MIT licence</a></li>
	</ul>
        <ul class="nav footer-refs pull-right">
          <li><a class="nm-pvm-questions" href="https://github.com/lorenwest/monitor-dashboard/issues"><i class="icon-question-sign"></i> Questions? </a></li>
          <li><a class="nm-pvm-comments" href="https://twitter.com/intent/tweet?source=webclient&text=%23NodeMonitor%20%23NodeJS" target="_blank"><i class="icon-bullhorn"></i> Comments <i class="icon-twitter"></i></a></li>
        </ul>
      </div>
    </footer>

    <!-- Scripts -->
    <%- @getBlock('scripts').add(@getUrl(["/vendor/jquery.js","/vendor/log.js","/vendor/modernizr.js","/vendor/twitter-bootstrap/js/bootstrap.js","/js/underscore.js","/js/backbone.js"])).toHTML() %>
  </body>
</html>
